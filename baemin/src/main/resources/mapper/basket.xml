<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="baemin.basket">
    <insert id="insertBasket">
        INSERT INTO bm_basket
        (
            menuName,
            menuPrice,
            storeIdn,
            userIdn
        )
        VALUES
        (
            #{menuName},
            #{menuPrice},
            #{storeIdn},
            #{userIdn}
        )
    </insert>
</mapper>